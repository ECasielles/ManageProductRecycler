<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mercacortex.manageproductrecycler">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProductActivity" />
        <activity android:name=".LoginRelative_Activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddProductActivity" />
        <activity android:name=".ManageProductActivity"/>
        <activity android:name=".AccountSettingsActivity" />
        <activity android:name=".GeneralSettingsActivity" />
    </application>

</manifest>